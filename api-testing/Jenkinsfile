pipeline {
    agent any

    stages {
        stage('Compile') {
            steps {
                dir('api-testing') { 
                    bat 'mvn clean compile'
                    }                    
               
            }
        }
        stage('Testing') {
            steps {
                   dir('api-testing'){
                      bat 'mvn clean test'
                   }              
            }
        }
      }
}
